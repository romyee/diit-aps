<div class="page-header">
  <h1>My Profile</h1>
</div>
<%= simple_form_for :user, :url => 'profile', :html => { :class => 'form-horizontal' } do |f| %>
	<%= f.input :first_name, :input_html => {:disabled => true} %>
	<%= f.input :last_name, :input_html => {:disabled => true} %>
	<%= f.input :email, :input_html => {:disabled => true} %>
	<%= f.input :official_id, :label => "DIIT ID", :input_html => {:disabled => true} %>
	<%= f.input :permanent_address, :input_html => {:size => "60x5", :disabled => true} %>
	<%= f.input :present_addresss, :input_html => {:size => "60x5", :disabled => true} %>
	<%= f.input :contact_number, :disabled => true %>
<% end %>

